<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >

<RelativeLayout
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.generalinfo.viewchampions" >

    <TextView
        android:id="@+id/nameDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginRight="68dp"
        android:layout_marginTop="19dp"
        android:text="Large Text"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    
    
    <TextView
        android:id="@+id/primaryDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="40dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />
    <TextView
        android:id="@+id/secondaryDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="50dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />
    <TextView
        android:id="@+id/attackDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="60dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />
    <TextView
        android:id="@+id/defenseDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="70dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />
    <TextView
        android:id="@+id/difficultyDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="80dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />
    <TextView
        android:id="@+id/magicAbilityDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="90dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />
    <TextView
        android:id="@+id/healthDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="100dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />
    <TextView
        android:id="@+id/healthRegenDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="110dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />
    <TextView
        android:id="@+id/manaDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="120dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />
    <TextView
        android:id="@+id/manaRegenDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="130dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />
    <TextView
        android:id="@+id/rangeDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="140dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />
    <TextView
        android:id="@+id/attackDamDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="150dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />
    <TextView
        android:id="@+id/attackSpeedDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="160dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />
    <TextView
        android:id="@+id/armorDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="170dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />
    <TextView
        android:id="@+id/magicResDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="180dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/titleDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/nameDis"
        android:layout_below="@+id/nameDis"
        android:text="Medium Text"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/movespeedDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/titleDis"
        android:layout_marginTop="190dp"
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall" />

     <ImageView
         android:id="@+id/icon"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignRight="@+id/titleDis"
         android:layout_below="@+id/attackDis"
         android:src="@drawable/clotharmor" />

     <ImageView
         android:id="@+id/spell0"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginTop="210dp"         
         android:layout_marginLeft="10dp"
         android:src="@drawable/blitzcrank0" />

     <ImageView
         android:id="@+id/spell2"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginTop="210dp"
         android:layout_marginLeft="110dp"
         android:src="@drawable/clotharmor" />

     <ImageView
         android:id="@+id/spell1"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginTop="210dp"         
         android:layout_marginLeft="60dp"         
         android:src="@drawable/clotharmor" />

     <ImageView
         android:id="@+id/spell3"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginTop="210dp"
         android:layout_marginLeft="160dp"         
         android:src="@drawable/clotharmor" />

     <ImageView
         android:id="@+id/spell4"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginTop="210dp"
         android:layout_marginLeft="210dp"
         android:src="@drawable/clotharmor" />

</RelativeLayout>
</ScrollView>
